snippet cl "console.log"
	console.log(${0});
snippet cll "console.log with label"
	console.log("$1", $1);
snippet cd "console.debug"
	console.debug(${0});
snippet ce "console.error"
	console.error(${0});
snippet cw "console.warn"
	console.warn(${0});
snippet ci "console.info"
	console.info(${0});
snippet ct "console.trace"
	console.trace(${0:label});


snippet desc
	describe('${1:MyThing}', () => {
		${0:it('does something', () => {}}
	})
snippet it
	it('${1:does something}', () => {
		$0
	}}
